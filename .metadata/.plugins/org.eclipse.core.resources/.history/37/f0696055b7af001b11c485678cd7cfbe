package test;

public class MyClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//----Printing string
		System.out.println("Hello world");
	//-----addition
		int n1=10;
		int n2=11;
		
		int sum=n1+n2;
		System.out.println("Addition of " + n1 + "and" + n2 + "is :"+sum);
		
		//--using for loop-----
		int i=0;
		for(i=0;i<=6;i++)
		{
			System.out.println(i);
			
		}
			
	}

}
